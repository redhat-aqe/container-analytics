{
  "name": "redhat-container-analytics",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@patternfly/react-charts": "5.1.5",
    "@patternfly/react-core": "3.120.5",
    "@patternfly/react-icons": "^3.14.19",
    "@patternfly/react-tokens": "2.7.5",
    "@types/lodash": "^4.14.146",
    "@types/node": "12.7.2",
    "@types/react": "16.9.2",
    "@types/react-dom": "16.9.0",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "prop-types": "^15.7.2",
    "react": "16.9.0",
    "react-dom": "16.9.0",
    "typescript": "3.5.3"
  },
  "scripts": {
    "start": "webpack --mode development --config webpack.config.js --progress --watch & live-server dist/",
    "test": "jest --ci",
    "lint": "tslint -p tsconfig.json",
    "build": "webpack --mode production --config webpack.config.js --progress --display-error-details"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/enzyme": "^3.10.3",
    "@types/enzyme-adapter-react-16": "^1.0.5",
    "@types/jest": "^24.0.18",
    "@types/prop-types": "^15.7.1",
    "@types/react-shadow-dom-retarget-events": "^1.0.0",
    "assets-webpack-plugin": "^3.9.10",
    "brotli-webpack-plugin": "^1.1.0",
    "compression-webpack-plugin": "^3.0.0",
    "css-loader": "^3.2.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.15.1",
    "html-webpack-plugin": "^3.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.8.0",
    "live-server": "^1.2.1",
    "mini-css-extract-plugin": "^0.8.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "react-hot-loader": "^4.12.12",
    "react-web-component": "2.0.1",
    "react-web-component-style-loader": "^0.1.4-alpha",
    "source-map-loader": "^0.2.4",
    "svg-url-loader": "^3.0.2",
    "terser-webpack-plugin": "^2.1.0",
    "ts-jest": "^24.0.2",
    "ts-loader": "^6.0.4",
    "tslint": "^5.20.0",
    "tslint-react": "^4.1.0",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "url-loader": "^2.1.0",
    "webpack": "4.39.3",
    "webpack-bundle-analyzer": "^3.5.2",
    "webpack-cli": "^3.3.7",
    "webpack-dev-server": "3.4.1"
  },
  "jest": {
    "collectCoverage": true,
    "coverageReporters": [
      "html",
      "text"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    },
    "moduleNameMapper": {
      "\\.(css|less)$": "identity-obj-proxy"
    },
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "setupFiles": [
      "<rootDir>/src/setupTests.js"
    ]
  }
}
